{
    "version": 3,
    "env": {
        "BASE_URI": "https://quran-data.vercel.app"
    },
    "builds": [
        {
            "src": "server/app.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "server/app.js"
        }
    ]
}